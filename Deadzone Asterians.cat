        <rule id="a12829d4-9d7b-5e35-b1a3-76759e3b6d5f" name="Energy Shield (X)" hidden="false" page="77">
          <description>When the model suffers potential damage, roll X dice. For each 6+, one potential damage is absorbed. If more 1&apos;s than successes are rolled, the shield is destroyed after it absorbs the damage.</description>
          <modifiers/>
        </rule>
      </rules>
      <profiles/>
      <links/>
    </entry>
    <entry id="25288a72-f280-2993-e7fb-29c4393c53d9" name="Frag Grenade" points="0.0" categoryId="(No Category)" type="upgrade" minSelections="0" maxSelections="-1" minPoints="0.0" maxPoints="-1.0" minInRoster="0" maxInRoster="-1" collective="false" page="77">
      <entries/>
      <entryGroups/>
      <modifiers/>
      <rules>
        <rule id="ccce232f-ce66-d808-4a06-87b775658fef" name="Frag Grenade" hidden="false" page="77">
          <description>Use Throw Grenade action and Frag ability to determine damage.</description>
          <modifiers/>
        </rule>
      </rules>
